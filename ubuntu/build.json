    "build_from": {
        "aarch64": "arm64v8/ubuntu:",
        "armv7": "arm32v7/ubuntu:",
        "amd64": "ubuntu:"
    },
    "args": {
        "BASHIO_VERSION": "0.13.1",
        "TEMPIO_VERSION": "2021.05.0",
        "S6_OVERLAY_VERSION": "2.1.0.2"
